    "axios": "^0.24.0",
    "concurrently": "^7.0.0",
    "flat": "^5.0.2",
    "form-data": "^4.0.0",
    "formik": "^2.2.9",
    "mailgun.js": "^4.1.4",
    "next": "^12.0.9",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-cookie-consent": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-google-recaptcha-v3": "^1.9.7",
    "react-icons": "^4.3.1",
