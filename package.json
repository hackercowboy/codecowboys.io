{
  "name": "codecowboys",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "build": "razzle build --noninteractive",
    "cypress": "concurrently --kill-others --success first \"yarn start\" \"cypress open\"",
    "cypress:run": "concurrently --kill-others --success first \"yarn start\" \"cypress run\"",
    "lint:fix": "eslint src --fix && stylelint src/**/*.scss --fix",
    "lint": "eslint src && stylelint src/**/*.scss",
    "eslint:fix": "eslint src --fix",
    "eslint": "eslint src",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "stylelint": "stylelint src/**/*.scss",
    "start:prod": "NODE_ENV=production node build/server.js",
    "start": "razzle start",
    "test": "razzle test --coverage --watch --testEnvironment=jsdom",
    "test:coverage": "razzle test --coverage --testEnvironment=jsdom"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "flat": "^5.0.2",
    "forcedomain": "^2.2.11",
    "form-data": "^4.0.0",
    "formik": "^2.2.9",
    "mailgun.js": "^4.1.4",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-cookie-consent": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-google-recaptcha-v3": "^1.9.7",
    "react-image-gallery": "^1.2.7",
    "react-intl": "^5.24.0",
    "react-key-listener": "^1.0.0",
    "react-parallax": "^3.3.0",
    "react-router-dom": "^6.2.1",
    "react-scroll": "^1.8.4",
    "validator": "^13.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-preset-razzle": "^4.2.13",
    "cypress": "^9.2.0",
    "enzyme": "^3.11.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "5.5.0",
    "jest-react-hooks-shallow": "^1.5.1",
    "mini-css-extract-plugin": "2.4.5",
    "npm": "^8.3.0",
    "postcss": "8.4.5",
    "razzle": "^4.2.13",
    "razzle-dev-utils": "^4.2.13",
    "razzle-plugin-mdx": "^4.2.13",
    "razzle-plugin-scss": "^4.2.13",
    "stylelint": "^14.2.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0",
    "webpack": "5.65.0",
    "webpack-dev-server": "4.7.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "jsdom"
  }
}
