    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.1",
    "cookieconsent": "^3.0.6",
    "dotenv": "^6.2.0",
    "emit-file-loader": "^0.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "forcedomain": "^1.0.1",
    "formik": "^0.11.11",
    "frisbee": "^1.6.4",
