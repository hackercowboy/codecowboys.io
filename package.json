    "react-lazy-load-image-component": "^1.5.1",
    "react-lazyload": "^3.2.0",
    "react-parallax": "^3.3.0",
    "react-router-dom": "^6.2.1",
    "react-scroll": "^1.8.4",
    "validator": "^13.7.0",
    "yup": "^0.32.11"
