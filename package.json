{
  "name": "codecowboys",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "cypress:run": "concurrently --kill-others --success first \"yarn dev\" \"cypress run\"",
    "cypress": "concurrently --kill-others --success first \"yarn dev\" \"cypress open\"",
    "dev": "next dev",
    "eslint:fix": "eslint src cypress --fix",
    "eslint": "eslint src cypress",
    "lint:fix": "eslint src --fix && stylelint src/**/*.scss --fix",
    "lint": "eslint src && stylelint src/**/*.scss",
    "postinstall": "yarn sitemap",
    "sitemap": "node bin/sitemap.js",
    "start:prod": "NODE_ENV=production node build/server.js",
    "start": "next start",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "stylelint": "stylelint src/**/*.scss",
    "test:coverage": "jest --coverage",
    "test": "jest --coverage --watch",
    "validate": "yarn lint && yarn test:coverage && yarn cypress:run"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@mdx-js/loader": "^1.6.22",
