    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "razzle": "^0.8.14",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-parallax": "^1.7.0",
    "react-router-dom": "^4.2.2",
