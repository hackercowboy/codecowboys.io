    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.3",
    "cookieconsent": "^3.1.0",
    "dotenv": "^6.2.0",
    "emit-file-loader": "^0.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
